#include<iostream>
#include<algorithm>
#include<string.h>
using namespace std;
#include <sstream>
#include <functional>

int main(){
	int a1[3],b1[3];

	for(int i=0;i<3;i++){
		cin>>a1[i];
	}
	for(int i=0;i<3;i++){
			cin>>b1[i];
		}

	int count_a=0,count_b=0;
	for(int i=0;i<3;i++){
			if(a1[i]>b1[i]){
				count_a++;
			}
			else if(a1[i]<b1[i]){
				count_b++;
			}
			else{
				//count_a=0,count_b=0;
			}
	}
	cout<<count_a<<" "<<count_b;


	return 0;
}
